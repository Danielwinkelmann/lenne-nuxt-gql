{
  "name": "@lenne.tech/lenne-nuxt-gql",
  "version": "1.0.0",
  "description": "Nuxt graphql module of @lenne.tech",
  "repository": "https://github.com/lenneTech/lenne-nuxt-gql",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "reinit": "rm -rf node_modules && rm -rf package-lock.json && npx nuxt cleanup && npm cache clean --force && npm i",
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "build": "nuxt-module-build",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.3",
    "defu": "^6.1.2",
    "graphql": "^16.6.0",
    "graphql-zeus": "^5.3.0",
    "ofetch": "^1.0.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.4.3",
    "@nuxt/test-utils": "^3.4.3",
    "@types/node": "^18",
    "changelogen": "^0.5.3",
    "eslint": "^8.39.0",
    "nuxt": "^3.4.3",
    "vitest": "^0.30.1"
  },
  "packageManager": "pnpm@7.29.1",
  "build": {
    "externals": [
      "graphql-zeus-core",
      "graphql-js-tree",
      "graphql"
    ]
  }
}
